<p id="notice"><%= notice %></p>

<h1>Contas</h1>

<table>
  <thead>
    <tr>
      <th>Usuario</th>
      <th>Saldo</th>
      <th>Numero</th>
      <th>Ativa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contas.each do |conta| %>
      <tr>
        <td><%= conta.usuario_id %></td>
        <td><%= conta.saldo %></td>
        <td><%= conta.numero %></td>
        <td><%= conta.ativa %></td>
        <td><%= link_to 'Show', conta %></td>
        <td><%= link_to 'Edit', edit_conta_path(conta) %></td>
        <td><%= link_to 'Destroy', conta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Conta', new_conta_path %>
